{
  "name": "pragmatic-web-boilerplate",
  "version": "1.0.1",
  "description": "Boilerplate for future PragmaticDLT website",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env.debug --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js",
    "generate": "plop --plopfile ./internals/generators/index.js",
    "prettier:js": "prettier --no-bracket-spacing --single-quote --prose-wrap never --write ./src/**/*.js",
    "prettier:jsx": "prettier --no-bracket-spacing --single-quote --prose-wrap never --write ./src/**/*.jsx"
  },
  "author": "Artem",
  "license": "MIT",
  "dependencies": {
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-react-devtools": "^4.2.15",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-simple-timefield": "^2.0.0",
    "styled-components": "^2.4.0",
    "thorify": "^0.3.1",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "plop": "^1.9.1",
    "prettier": "^1.10.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.2"
  }
}
